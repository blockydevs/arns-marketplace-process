# Process IDs and Token Configuration
# These are the core process IDs and token addresses for the ARnS Marketplace

# Activity tracking process ID - handles order history and activity reporting
ACTIVITY_PROCESS_ID=

# ARIO token process ID - the native token used in the marketplace
ARIO_TOKEN_PROCESS_ID=

# Default swap token ID - used as the default token for swaps (same as ARIO in this case)
DEFAULT_SWAP_TOKEN_ID=

# Trusted message configuration
# Owner address that should be blocked from sending messages to the marketplace
TRUSTED_MESSAGE_BLACKLIST_OWNER=